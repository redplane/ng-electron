{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "gulp --mode=development",
    "build": "gulp --mode=production"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "author": "GitHub",
  "email": "redplane_dt@yahoo.com.vn",
  "license": "CC0-1.0",
  "build": {
    "appId": "com.personal-cv.001",
    "productName": "personal-cv",
    "files": [
      {
        "from": "build",
        "filter": [
          "!resources/*"
        ]
      }
    ],
    "linux": {
      "category": "Development",
      "target": [
        "AppImage"
      ],
      "icon": "assets/icons/app.ico"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icons/app.ico"
    },
    "extraResources": [
      {
        "from": "build/resources",
        "to": "resources"
      }
    ]
  },
  "devDependencies": {
    "@uirouter/angularjs": "^1.0.17",
    "angular": "^1.7.2",
    "angular-block-ui": "^0.2.2",
    "angular-confirm1": "^1.1.0",
    "angular-file-upload": "^2.5.0",
    "angular-messages": "^1.7.0",
    "angular-moment": "^1.2.0",
    "angular-moment-picker": "^0.10.2",
    "angular-sanitize": "^1.7.2",
    "angular-toastr": "^2.1.1",
    "angular-translate": "^2.17.1",
    "angular-translate-loader-static-files": "^2.17.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.0.0-alpha.6",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "electron": "^2.0.6",
    "electron-builder": "^20.26.1",
    "electron-connect": "^0.6.3",
    "electron-rebuild": "^1.8.2",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-dest": "^0.2.3",
    "gulp-fn": "0.0.3",
    "gulp-install": "^1.1.0",
    "gulp-json-editor": "^2.4.1",
    "gulp-sequence": "^1.0.0",
    "gulp-watch": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "jquery-mousewheel": "^3.1.13",
    "lodash": "^4.17.10",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "moment": "^2.22.1",
    "ng-annotate-webpack-plugin": "^0.3.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.9.0",
    "oclazyload": "^1.1.0",
    "popper.js": "^1.14.3",
    "pusher-client": "^1.1.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "tether": "^1.4.4",
    "typescript": "^3.0.1",
    "ui-bootstrap4": "^3.0.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.4",
    "webpack-clean-obsolete-chunks": "^0.4.0",
    "webpack-cli": "^3.1.0",
    "webpack-stream": "^4.0.3",
    "yargs": "^11.0.0"
  },
  "dependencies": {}
}
